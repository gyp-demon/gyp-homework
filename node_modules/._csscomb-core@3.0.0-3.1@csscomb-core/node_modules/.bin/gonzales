#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../._gonzales-pe@3.0.0-28@gonzales-pe/bin/gonzales.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../._gonzales-pe@3.0.0-28@gonzales-pe/bin/gonzales.js" "$@"
  ret=$?
fi
exit $ret
